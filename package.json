{
    "name": "javascript-development-environment",
    "version": "1.0.0",
    "description": "JavaScript development environment for easily starting new projects",
    "scripts": {
        "prestart": "babel-node buildScripts/startMessage.js",
        "open:src": "babel-node buildScripts/srcServer.js",
        "start": "npm-run-all --parallel open:src lint:watch test:watch mock-data start-mockapi",
        "lint": "esw webpack.config.* src buildScripts --color",
        "lint:watch": "npm run lint -- --watch",
        "localtunnel": "lt --port 3000",
        "share": "npm-run-all --parallel start localtunnel",
        "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
        "test:watch": "npm run test -- --watch",
        "mock-data": "babel-node buildScripts/generateMockData",
        "start-mockapi": "json-server --watch src/api/db.json --port 3001",
        "clean-dist": "rimraf ./dist && mkdir dist",
        "prebuild": "npm-run-all clean-dist test lint",
        "build": "babel-node buildScripts/build.js",
        "postbuild": "babel-node buildScripts/distServer.js"
    },
    "author": "Reid E.",
    "license": "MIT",
    "dependencies": {
        "whatwg-fetch": "3.6.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.1",
        "@babel/core": "7.13.8",
        "@babel/node": "^7.13.0",
        "@babel/preset-env": "7.13.8",
        "@babel/register": "7.13.8",
        "@types/mini-css-extract-plugin": "^2.5.1",
        "babel-loader": "8.2.2",
        "chai": "4.3.0",
        "chalk": "4.1.0",
        "cheerio": "^1.0.0-rc.12",
        "compression": "1.7.4",
        "cross-env": "7.0.3",
        "css-loader": "5.1.0",
        "eslint": "7.21.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-watch": "7.0.0",
        "express": "^4.19.2",
        "faker": "^5.4.0",
        "html-webpack-plugin": "5.2.0",
        "jsdom": "^16.4.0",
        "json-schema-faker": "0.5.0-rcv.27",
        "json-server": "^1.0.0-beta.1",
        "localtunnel": "^1.8.3",
        "mini-css-extract-plugin": "^1.6.2",
        "mocha": "^10.5.2",
        "npm-run-all": "4.1.5",
        "numeral": "2.0.6",
        "open": "7.4.2",
        "rimraf": "3.0.2",
        "style-loader": "2.0.0",
        "surge": "^0.0.0",
        "webpack": "^5.92.1",
        "webpack-dev-middleware": "^7.2.1"
    }
}
